<%= Metasploit::Credential::Exporter::Pwdump::FILE_ID_STRING %> <%= @version_string %>
# Generated: <%= Time.now.utc %>
# Project: <%= @workspace.name %>
#
#########################################################

<% if data[:ntlm].present? %>
#  LM/NTLM Hashes (<%= data[:ntlm].size %> hashes, <%= service_count_for_hashes(data[:ntlm]) %> services)
  <% data[:ntlm].each do |login| %>
# <%= format_service_for_login(login) %>
<%= format_ntlm_hash(login) %>
  <% end %>
<% end %>

<% if data[:non_replayable].present? %>
#  Hashes (<%= data[:non_replayable].size %> hashes, <%= service_count_for_hashes(data[:non_replayable]) %> services)
  <% data[:non_replayable].each do |login| %>
# <%= format_service_for_login(login) %>
<%= format_nonreplayable_hash(login) %>
  <% end %>
<% end %>

<% if data[:password].present? %>
#  Plaintext Passwords (<%=  data[:password].size %> passwords, <%= service_count_for_hashes(data[:password]) %> services)
  <% data[:password].each do |login| %>
# <%= format_service_for_login(login) %>
<%= format_password(login) %>
  <% end %>
<% end %>

<% if data[:postgres_md5].present? %>
#  Postgres MD5 Hashes (<%=  data[:postgres_md5].size %> hashes, <%= service_count_for_hashes(data[:postgres_md5]) %> services)
  <% data[:postgres_md5].each do |login| %>
# <%= format_service_for_login(login) %>
<%= format_postgres_md5(login) %>
  <% end %>
<% end %>